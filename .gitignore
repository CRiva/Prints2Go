# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

/db/data.yml

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore Byebug command history file.
.byebug_history

# Ignore Uploaded files
/public/uploads/*

# Ignore secret key
/config/secrets.yml
config/secrets.yml
secrets.yml

# Ignore mysql credentials
/config/mysql_database.yml
/config/database.yml
config/database.yml

$ Ignore DS_store
*.DS_Store
.DS_Store

/db/see*

/public/assets/*
/app/assets/fonts/*
/app/assets/images/*

config.database.yml.sqlite3